{
  "$schema": "https://typedoc.org/schema.json",
  "out": "../docs/docs/isaacscript-common",
  "sort": ["source-order"],
  "tsconfig": "tsconfig.json",
  "readme": "website-root.md",
  "treatWarningsAsErrors": true,
  "validation": {
    "invalidLink": true,
    "notExported": true,
  },
  "entryPoints": ["src/indexTypeDoc.ts"],
  "intentionallyNotExported": [
    "AddCallbackParameterCustom",
    "ImmutablePrimitive",
    "ImmutableArray",
    "ImmutableMap",
    "ImmutableSet",
    "ImmutableObject",
  ],
  "plugin": [
    "@zamiell/typedoc-plugin-markdown",
    "@zamiell/typedoc-plugin-not-exported",
  ],
  "githubPages": false,
  "hideBreadcrumbs": true,
  "hideInPageTOC": true,
}

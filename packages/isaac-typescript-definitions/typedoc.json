{
  "$schema": "https://typedoc.org/schema.json",
  "out": "../docs/docs/isaac-typescript-definitions",
  "sort": ["source-order"],
  "tsconfig": "tsconfig.json",
  "readme": "website-root.md",
  "treatWarningsAsErrors": false,
  "validation": {
    "invalidLink": true,
    "notExported": true
  },
  "entryPoints": "src/index.ts",
  "entryPointStrategy": "Expand",
  "intentionallyNotExported": [
    "ActionTriggerType",
    "ActionTriggerValue",
    "BitFlags",
    "CacheFlagType",
    "CacheFlagValue",
    "DamageFlagType",
    "DamageFlagValue",
    "DisplayFlagType",
    "DisplayFlagValue",
    "DoorSlotFlagType",
    "DoorSlotFlagValue",
    "EntityFlagType",
    "EntityFlagValue",
    "EntityPartitionType",
    "EntityPartitionValue",
    "ItemConfigTagType",
    "ItemConfigTagValue",
    "LevelCurseType",
    "LevelCurseValue",
    "ProjectileFlagType",
    "ProjectileFlagValue",
    "RoomDescriptorFlagType",
    "RoomDescriptorFlagValue",
    "TargetFlagType",
    "TargetFlagValue",
    "TearFlagType",
    "TearFlagValue",
    "UseFlagType",
    "UseFlagValue"
  ],
  "plugin": ["typedoc-plugin-markdown", "@zamiell/typedoc-plugin-not-exported"],
  "hideBreadcrumbs": true,
  "hideInPageTOC": true,
  "hidePageTitle": true
}

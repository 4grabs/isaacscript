{
  "name": "eslint-plugin-isaacscript",
  "version": "2.3.1",
  "description": "An ESLint plugin that contains useful rules.",
  "keywords": [
    "eslint",
    "plugin",
    "isaacscript",
    "lint",
    "linting",
    "prettier",
    "rules",
    "enum",
    "enums",
    "comment",
    "comments",
    "format",
    "autoformat",
    "complete",
    "sentences"
  ],
  "homepage": "https://isaacscript.github.io/",
  "bugs": {
    "url": "https://github.com/IsaacScript/isaacscript/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/IsaacScript/isaacscript.git"
  },
  "license": "MIT",
  "author": "Zamiell",
  "type": "commonjs",
  "main": "src/index.js",
  "scripts": {
    "generate": "npx ts-node tools/scripts/generate.mts",
    "generate:configs": "npx ts-node tools/scripts/generate-configs.ts",
    "generate:readme": "npx ts-node tools/scripts/generate-readme.ts",
    "generate:rules": "npx ts-node tools/scripts/generate-rules.ts",
    "test": "jest"
  },
  "peerDependencies": {
    "@typescript-eslint/parser": ">= 5.0.0",
    "eslint": ">= 8.0.0",
    "typescript": ">= 4.0.0"
  }
}
